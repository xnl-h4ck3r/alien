PARAMLENGTH=${#PARAM}
LINELENGTH=$(($PARAMLENGTH / 4))
if [ ${LINELENGTH} -lt 10 ]; then
	LINELENGTH=10
fi

IFS=' ' read -ra my_array <<< "$PARAM"
for i in "${my_array[@]}"
do
	if [ ${#LINE1} -lt $LINELENGTH ]; then
		LINE1="$LINE1${i} "
	else
		if [ ${#LINE2} -lt $LINELENGTH ]; then
			LINE2="$LINE2${i} "
		else
			if [ ${#LINE3} -lt $LINELENGTH ]; then
				LINE3="$LINE3${i} "
			else
				LINE4="$LINE4${i} "
			fi
		fi
	fi
done


if [ -z "$LINE4" ]; then
	LINE4="GRRRRRRRRR!"
fi

LONGESTLINE=${#LINE1}
if [ ${#LINE2} -gt ${LONGESTLINE} ]; then
	LONGESTLINE=${#LINE2}
fi
if [ ${#LINE3} -gt ${LONGESTLINE} ]; then
	LONGESTLINE=${#LINE3}
fi
if [ ${#LINE4} -gt ${LONGESTLINE} ]; then
	LONGESTLINE=${#LINE4}
fi


TAGA=$(printf %-"$((LONGESTLINE))"s | tr " " "-")
TAGB=$(printf %-"$((LONGESTLINE))"s)
LINE1=${LINE1}$(printf %-"$(( ${#LINE1} - LONGESTLINE ))"s)"  |"
LINE2=${LINE2}$(printf %-"$(( ${#LINE2} - LONGESTLINE ))"s)"  |"
LINE3=${LINE3}$(printf %-"$(( ${#LINE3} - LONGESTLINE ))"s)"  |"
LINE4=${LINE4}$(printf %-"$(( ${#LINE4} - LONGESTLINE ))"s)"  |"
TAGC=$(printf %-"$((LONGESTLINE))"s)
TAGD=$(printf %-"$(( LONGESTLINE - 5 ))"s | tr " " "-")

sed "s/>A</${TAGA}./; s/>B</${TAGB}\\\/; s/>C</${TAGC}\//; s/>D</${TAGD}\'/; s/>V</${LINE1}/; s/>X</${LINE2}/; s/>Y</${LINE3}/; s/>Z</${LINE4}/" <<< $alien



